{
  "name": "any-qs",
  "version": "1.2.1",
  "description": "parse any query parameters from url",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run dist",
    "uglify": "uglifyjs --compress --mangle -o dist/any-qs.min.js --source-map dist/any-qs.min.map -- dist/any-qs.js",
    "dist": "npm run clean && cp index.js dist/any-qs.js && npm run uglify",
    "test": "nyc --reporter text --reporter html mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf dist && mkdir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeluoqiuzhi/any-qs.git"
  },
  "author": "yeluoqiuzhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yeluoqiuzhi/any-qs/issues"
  },
  "homepage": "https://github.com/yeluoqiuzhi/any-qs#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "mocha": "^3.4.1",
    "nyc": "^10.3.2",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "uglify-js": "^3.0.24"
  }
}
